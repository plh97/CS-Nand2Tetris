 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=aa);
    Not(in=b, out=bb);
    Nand(a=aa, b=bb, out=out);
    // Put your code here:
}
